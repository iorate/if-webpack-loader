{
  "author": "iorate",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/loader-utils": "^2.0.1",
    "@types/webpack": "^4.41.24",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "fibers": "^5.0.0",
    "jest": "^26.6.3",
    "loader-utils": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "semantic-release": "^17.2.2",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "description": "Conditional compilation for webpack.",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "LICENSE.txt",
    "README.md"
  ],
  "jest": {
    "preset": "ts-jest"
  },
  "keywords": [
    "conditional",
    "if",
    "ifdef",
    "javascript",
    "loader",
    "typescript",
    "webpack"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "if-webpack-loader",
  "peerDependencies": {
    "loader-utils": "^2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": "github:iorate/if-webpack-loader",
  "scripts": {
    "build": "tsc",
    "commit": "git-cz",
    "lint": "run-p --print-label lint:*",
    "lint:eslint": "eslint --color .",
    "lint:prettier": "prettier --color --loglevel warn --check .",
    "lintfix": "run-s --print-label lintfix:*",
    "lintfix:eslint": "eslint --color --fix .",
    "lintfix:prettier": "prettier --color --loglevel warn --write .",
    "test": "jest"
  },
  "typings": "dist/index.d.ts",
  "version": "0.1.0"
}
